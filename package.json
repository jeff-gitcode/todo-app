{
  "name": "todo-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "build:analyze": "yarn build && source-map-explorer .next/static/**/*.js",
    "start": "next start",
    "export": "next build && next export",
    "start:build": "cp -r .next/static .next/standalone/.next/static && cp -r public .next/standalone/public && node .next/standalone/server.js",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format:fix": "prettier --write --list-different .",
    "format:check": "prettier --check .",
    "format:diff": "prettier --list-different .",
    "lint-staged": "lint-staged",
    "test": "jest",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "test:storybook": "test-storybook --url http://localhost:6006",
    "test:storybook:ci": "test-storybook --url http://localhost:6006 --ci",
    "test:storybook:coverage": "test-storybook --url http://localhost:6006 --coverage",
    "test:storybook:report": "test-storybook --url http://localhost:6006 --reporter dot",
    "test:storybook:report:ci": "test-storybook --url http://localhost:6006 --reporter dot --ci",
    "test:storybook:ui": "test-storybook --url http://localhost:6006 --ui",
    "test:storybook:ui:ci": "test-storybook --url http://localhost:6006 --ui --ci",
    "test:playwright": "playwright test",
    "test:playwright:ci": "playwright test --ci",
    "test:playwright:coverage": "playwright test --coverage",
    "test:playwright:report": "playwright test --reporter dot",
    "test:playwright:report:ci": "playwright test --reporter dot --ci",
    "test:playwright:ui": "playwright test --ui",
    "test:playwright:ui:ci": "playwright test --ui --ci"
  },
  "type": "module",
  "dependencies": {
    "@auth/core": "^0.38.0",
    "@auth/prisma-adapter": "^2.7.4",
    "@hookform/resolvers": "^4.1.3",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.3.0",
    "@radix-ui/react-avatar": "^1.1.2",
    "@radix-ui/react-checkbox": "^1.1.3",
    "@radix-ui/react-dropdown-menu": "^2.1.5",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-navigation-menu": "^1.2.4",
    "@radix-ui/react-slot": "^1.1.1",
    "@storybook/react": "^8.6.3",
    "@tanstack/react-query": "^5.65.1",
    "@tanstack/react-query-devtools": "^5.65.1",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "jest": "^29.7.0",
    "lucide": "^0.477.0",
    "lucide-react": "^0.477.0",
    "next": "15.1.6",
    "next-auth": "5.0.0-beta.25",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "shadcn-ui": "^0.9.4",
    "tailwind-merge": "^3.0.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@babel/core": "^7.26.9",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.26.0",
    "@chromatic-com/storybook": "3.2.5",
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.51.0",
    "@storybook/addon-actions": "^8.6.4",
