    "prettier": "^3.5.3",
    "prisma": "^6.3.0",
    "react-test-renderer": "^19.0.0",
    "storybook": "8.6.4",
    "storybook-addon-module-mock": "^1.3.4",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.2",
    "vitest": "^3.0.7",
    "whatwg-fetch": "^3.6.20"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
